}

.create {
    overflow: hidden;
    border-top: 20rpx solid #f0f0f6;
    border-bottom: 2rpx solid #f0f0f6;
    padding: 0 30rpx;
}

.create .text {
    font-size: 32rpx;
    font-weight: 600;
    color: #1b1b1b;
    height: 88rpx;
    line-height: 88rpx;
}

.template1_box {
    font-size: 28rpx;
    color: #2e2e2e;
}

.template1_box .template1 {
