    height: 88rpx;
    line-height: 88rpx;
    border-bottom: 2rpx solid #f0f0f6;
}

.text_box {
    padding: 40rpx 30rpx;
}

.text_box .t1,
.text_box .t2 {
    font-size: 28rpx;
    color: #2e2e2e;
    line-height: 40rpx;
}

.text_box .t1 {
